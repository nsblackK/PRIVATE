    "file=open(\"Several Lines\",\"r\")\n",
    "L=file.readlines()\n",
    "c=0\n",
    "c=len(L)\n",
    "file.close()\n",
    "\n",
    "file2=open(\"Several Lines\", mode=\"w\")\n",
    "file2.write(\"\")\n",
    "file2.close()\n",
    "\n",
    "file3=open(\"Several Lines\", mode=\"a\")\n",
    "for j in range(1,c):\n",
    "    file3.write(str(j)+\".\"+L[j])\n",
    "\n",
    "\n"
